{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "cd frontend && npm install && npm run vercel-build",
  "outputDirectory": "frontend/.next",
  "builds": [
    {
      "src": "frontend/next.config.js",
      "use": "@vercel/next"
    }
  ],
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./frontend/pages/"
} 