{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "cd frontend && npm install --force && npm run vercel-build",
  "outputDirectory": "frontend/.next",
  "builds": [
    {
      "src": "frontend/next.config.js",
      "use": "@vercel/next",
      "config": {
        "skipBuildCache": true,
        "zeroConfig": false
      }
    }
  ],
  "installCommand": "npm install --force"
} 